{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "functions": {
    "api/messages.js": {
      "maxDuration": 300
    },
    "api/stream.js": {
      "maxDuration": 300
    }
  },
  "rewrites": [
    { "source": "/api/stream", "destination": "/api/stream" },
    { "source": "/api/messages", "destination": "/api/messages" },
    { "source": "/api/screen-sanctions", "destination": "/api/screen-sanctions" },
    { "source": "/api/ownership-network", "destination": "/api/ownership-network" }
  ]
}
